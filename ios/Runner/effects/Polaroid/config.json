{
  "name": "Polaroid80s",
  "frx": {
    "type": "3d_anim",
    "3d_anim": {
      "type": "vfx"
    }
  },
  "foreground" : {
    "type" : "MEDIA",
    "media" : {
      "type" : "VIDEO",
      "file" : "polaroid.mp4",
      "videoSpeed" : 1.0,
      "blend" : "SCREEN",
      "mode" : "RGBA"
    }
  },
  "camera": {
      "type": "MEDIA",
      "showWithFaceOnly": "true",
      "media": {
          "type": "PICTURE",
          "file": "noise.png",
          "opacity": 0.2,
          "blend": "MULTIPLY"
      }
  },
  "final": {
    "type": "FILTER",
    "filter": {
      "fragmentCode": "finalColorFilter.fsh",
      "textures": [{
        "file": "polaroid.png",
        "sampler": "lookupTexture",
        "magFilter": "LINEAR",
        "minFilter": "LINEAR"
      }]
    }
  },
  "sounds" : [
                  { "trackName" : "music.ogg" }
                  ],
  "recordDuration": 15
}